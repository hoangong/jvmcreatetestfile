{
  "name": "jvmcreatetestfile",
  "displayName": "Test file creator for Java and Scala",
  "description": "Test file creator for Java and Scala",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jvmcreatetestfile.createTestFile",
        "title": "Create Test file"
      }
    ],
    "menus": {
        "editor/context": [
            {
                "when": "resourceLangId == java || resourceLangId == scala",
                "command": "jvmcreatetestfile.createTestFile",
                "group": "navigation"
            }
        ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
